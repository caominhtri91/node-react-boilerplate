{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Node/React boilerplate",
  "main": "index.js",
  "scripts": {
      "dev:server": "nodemon ./server/index.js",
      "dev:client": "cd ./client && webpack --mode development --config ./webpack.client.js --watch",
      "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\""
  },
